{
    "name": "alaphasnow/opencc-php",
    "description": "OpenCC made with PHP",
    "keywords": ["OpenCC", "PHP"],
    "license": "MIT",
    "authors": [
        {
            "name": "alaphasnow",
            "email": "wind91@foxmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/process": "^4.3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8"
    },
    "autoload": {
        "psr-4": {
            "AlaphaSnow\\OpenCC\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
          "AlaphaSnow\\OpenCC\\Test\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlaphaSnow\\OpenCC\\ServiceProvider"
            ],
            "aliases": {
                "OpenCC": "Facade"
            }
        }
    }
}
